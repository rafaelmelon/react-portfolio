    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.8",
    "@types/react-dom": "^16.0.11",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
