{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "jsx-no-lambda": false
  }
}
